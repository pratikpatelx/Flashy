String createWard = " CREATE TABLE Wards ( "
  + " wardID INTEGER, "
  + " wardOffice VARCHAR(60), "
  + " PRIMARY KEY (wardID)); ";


String createCouncilor = " CREATE TABLE Councilors ( "
  + " cID INTEGER IDENTITY, "
  + " wardID INTEGER, "
  + " councilMbr VARCHAR(60), "
  + " PRIMARY KEY (cID), "
  + " FOREIGN KEY (wardID) REFERENCES Wards); ";

  String createExpense = " CREATE TABLE Expenses ( "
   + " eID INTEGER IDENTITY, "
   + " cID INTEGER, "
   + " jDate VARCHAR(60), "
   + " vendor VARCHAR(60), "
   + " eType VARCHAR(60), "
   + " description VARCHAR(60), "
   + " acct VARCHAR(60), "
   + " amt DECIMAL, "
   + " PRIMARY KEY (eID), "
   + " FOREIGN KEY (cID) REFERENCES Councilors); ";
